# ─────────────── Security / Crypto ───────────────
ENCRYPTION_KEY=
ENCRYPTION_KEYS=${ENCRYPTION_KEY}
APP_SIGNING_KEY=dev_signing_key

# ─────────────── App / URLs ───────────────
BASE_URL=https://${CODESPACE_NAME}-8000.app.github.dev
DEFAULT_TENANT_NAME=Matty CRE (Dev)

# ─────────────── Kixie ───────────────
KIXIE_API_KEY=
KIXIE_BUSINESS_ID=
# dev: empty to skip signature in Swagger tests
KIXIE_WEBHOOK_SECRET=
KIXIE_SIGNATURE_HEADER=X-Kixie-Signature
# KIXIE_API_BASE=https://apig.kixie.com/app/v1/api

# ─────────────── RealNex ───────────────
REALNEX_JWT=
REALNEX_API_BASE=https://sync.realnex.com/api/v1/Crm
RN_PROJECT_KEY=

# Event/Status keys (these names match the code)
RN_EVENTTYPEKEY_DEFAULT=1
RN_EVENTTYPEKEY_CALL=1
RN_STATUSKEY_DEFAULT=0

# History link field order — your tenant wants leadKey
RN_HISTORY_CONTACT_LINK_FIELDS=leadKey,partyKey,contactKey,linkedTo

# ─────────────── Database ───────────────
DATABASE_URL=sqlite:///./goose.db
DB_CREATE_ALL=1

# ─────────────── Server Hardening (optional) ───────────────
LOG_LEVEL=INFO
CORS_ALLOW_ORIGINS=*
TRUSTED_HOSTS=*

